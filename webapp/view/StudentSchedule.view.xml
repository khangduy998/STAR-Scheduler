<mvc:View controllerName="STARSchedule.STARSchedule.controller.StudentSchedule" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:tnt="sap.tnt"
	displayBlock="true" xmlns="sap.m">
	<App id="idAppControl">
		<pages>
			<Page title="{Managers>Name}" showNavButton="true"
			navButtonPress="onNavBack">
				<content>
					<Table id="Monday"
					items="{Managers>Monday}">
						<headerToolbar>
							<Toolbar style="Clear">
								<ToolbarSpacer>
									
								</ToolbarSpacer>
								<Title text="Monday" level="H2"/>
								<ToolbarSpacer>
									
								</ToolbarSpacer>
								
							</Toolbar>
						</headerToolbar>
						<columns>
							<Column  demandPopin="true"><Text text="Start Time"/></Column>
							<Column  demandPopin="true"><Text text="End Time"/></Column>
						</columns>
						<items>
							<ColumnListItem>
								<cells>
									<Text text="{Managers>Start}"/>
									<Text text="{Managers>End}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
					<Table id="Tuesday"
