{
	"_version": "1.8.0",
	"sap.app": {
		"id": "STARSchedule.STARSchedule",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "STARSchedule.STARSchedule.view.View1",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "STARSchedule.STARSchedule.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "STARSchedule.STARSchedule.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteView1",
					"pattern": "RouteView1",
					"target": [
						"TargetView1"
					]
				},
				{
					"name": "LogInView",
					"pattern": "LogIn",
					"target": [
						"login"
					]
				},
				{
					"name": "StudentSchedule",
					"pattern": "Managers/:Muser:/Intern/:Iuser:",
					"target": [
						"StudentSchedule"
					]
				},
				{
					"name": "ManagerInternTable",
					"pattern": "ManagerInternTable/:type:",
					"target": [
						"ManagerInternTable"
					]
				},
				{
					"name": "AddStudentForm",
					"pattern": "AddStudentForm",
					"target": [
						"AddStudentForm"
					]
				}
				
			],
			"targets": {
				"TargetView1": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View1",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "STARSchedule.STARSchedule.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl"
				},
				"login": {
					"viewType": "XML",
					"viewName": "login"
				},
				"StudentSchedule": {
					"viewType": "XML",
					"viewName": "StudentSchedule"
				},
				"ManagerInternTable": {
					"viewType": "XML",
					"viewName": "ManagerInternTable"
				},
				"AddStudentForm": {
					"viewType": "XML",
					"viewName": "AddStudentForm"
				}
				
				
			}
		}
	}
}
